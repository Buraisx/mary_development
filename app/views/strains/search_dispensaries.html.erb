<h2>Find a dispensary near you!</h2>
<%= form_tag dispensaries_path, :method => :get do %>
  <p>
    <%= label_tag :search, 'Address' %>
    <%= text_field_tag :search, params[:search], placeholder: "ex: Toronto Canada" %>
    <%= label_tag :search, 'Within distance (miles)' %>
    <%= number_field_tag :miles, params[:miles], placeholder: "ex: 5"%>
    <br>
    <%= submit_tag "enter" %>
  </p>
<% end %>
